Sub AphabticalTesting()        ' Loop through all sheets    Dim Worksheet As Worksheet    For Each Worksheet In ActiveWorkbook.Worksheets    Worksheet.Activate        ' Determine the Last Row        LastRow = Worksheet.Cells(Rows.Count, 1).End(xlUp).Row        ' Insert Heading for summary        Cells(1, "I").Value = "Ticker"        Cells(1, "J").Value = "Yearly Change"        Cells(1, "K").Value = "Percent Change"        Cells(1, "L").Value = "Total Stock Volume"                'Create the variables to hold Value        Dim Open_Price As Double        Dim Close_Price As Double        Dim Yearly_Change As Double        Dim Ticker_Name As String        Dim Percent_Change As Double        Dim Volume As Double        Volume = 0        Dim Row As Double        Row = 2        Dim Column As Integer        Column = 1        Dim i As Long                'Set Initial stock Open Price        Open_Price = Cells(2, Column + 2).Value         ' Loop through all ticker symbol                For i = 2 To LastRow                 ' Check if the ticker symbol still the same, if it is not the same:            If Cells(i + 1, Column).Value <> Cells(i, Column).Value Then                            ' Set the Ticker name                Ticker_Name = Cells(i, Column).Value                Cells(Row, Column + 8).Value = Ticker_Name                                ' Set the stock Close Price                Close_Price = Cells(i, Column + 5).Value                                ' Calculate Yearly Change for the stock                Yearly_Change = Close_Price - Open_Price                Cells(Row, Column + 9).Value = Yearly_Change                                ' Calculate Percent Change for the stock                If (Open_Price = 0 And Close_Price = 0) Then                    Percent_Change = 0                ElseIf (Open_Price = 0 And Close_Price <> 0) Then                    Percent_Change = 1                Else                    Percent_Change = Yearly_Change / Open_Price                    Cells(Row, Column + 10).Value = Percent_Change                    Cells(Row, Column + 10).NumberFormat = "0.00%"                End If                                ' Insert Total Volume of the stock                Volume = Volume + Cells(i, Column + 6).Value                Cells(Row, Column + 11).Value = Volume                                ' Add one to the summary table row                Row = Row + 1                                ' Reset the Open Price                Open_Price = Cells(i + 1, Column + 2)                                ' Reset the Volume Total                Volume = 0                                            'if cells are the same ticker            Else                Volume = Volume + Cells(i, Column + 6).Value            End If        Next i                ' Determine the Last Row of Yearly Change per each Worksheet        YearlyChangeLastRow = Worksheet.Cells(Rows.Count, Column + 8).End(xlUp).Row                ' Set Cell Colors        For j = 2 To YearlyChangeLastRow            If (Cells(j, Column + 9).Value > 0 Or Cells(j, Column + 9).Value = 0) Then                Cells(j, Column + 9).Interior.ColorIndex = 10            ElseIf Cells(j, Column + 9).Value < 0 Then                Cells(j, Column + 9).Interior.ColorIndex = 3            End If        Next j                    ' Look through each rows to find the greatest value and the associate ticker        For N = 2 To YearlyChangeLastRow            If Cells(N, Column + 10).Value = Application.WorksheetFunction.Max(Worksheet.Range("K2:K" & YearlyChangeLastRow)) Then                Cells(2, Column + 15).Value = Cells(N, Column + 8).Value                Cells(2, Column + 16).Value = Cells(N, Column + 10).Value                Cells(2, Column + 16).NumberFormat = "0.00%"            ElseIf Cells(N, Column + 10).Value = Application.WorksheetFunction.Min(Worksheet.Range("K2:K" & YearlyChangeLastRow)) Then                Cells(3, Column + 15).Value = Cells(N, Column + 8).Value                Cells(3, Column + 16).Value = Cells(N, Column + 10).Value                Cells(3, Column + 16).NumberFormat = "0.00%"            ElseIf Cells(N, Column + 11).Value = Application.WorksheetFunction.Max(Worksheet.Range("L2:L" & YearlyChangeLastRow)) Then                Cells(4, Column + 15).Value = Cells(N, Column + 8).Value                Cells(4, Column + 16).Value = Cells(N, Column + 11).Value            End If        Next N                  ' Set Greatest % Increase, % Decrease, and Total Volume for the stocks        Cells(2, Column + 14).Value = "Greatest % Increase"        Cells(3, Column + 14).Value = "Greatest % Decrease"        Cells(4, Column + 14).Value = "Greatest Total Volume"        Cells(1, Column + 14).Value = "Description"        Cells(1, Column + 15).Value = "Ticker"        Cells(1, Column + 16).Value = "Value"        Next Worksheet        End Sub